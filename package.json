{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "A quote bot for discord using ZenQuotes",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch ./src/server.ts",
    "build": "tsc -p tsconfig.json && tsc-alias",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "register-commands": "tsx ./src/scripts/registerSlashCommands.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "prisma": "^6.0.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@types/node": "^22.7.5",
    "tsc-alias": "^1.8.15",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
